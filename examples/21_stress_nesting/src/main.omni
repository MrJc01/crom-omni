fn print(msg: string) {
    native "js" { console.log(msg); }
    native "python" {
print(msg)
    }
}

fn deep_call_1() -> string { return deep_call_2(); }
fn deep_call_2() -> string { return deep_call_3(); }
fn deep_call_3() -> string { return deep_call_4(); }
fn deep_call_4() -> string { return deep_call_5(); }
fn deep_call_5() -> string { return "Deep reached!"; }

fn nested_conditionals(n: i64) -> string {
    if (n > 10) {
        if (n > 20) {
            if (n > 30) {
                return "very high";
            } else {
                return "high";
            }
        } else {
            return "medium";
        }
    } else {
        return "low";
    }
}

