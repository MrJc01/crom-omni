    print("╚══════════════════════════════════════╝");
    print("");
    
    print("1. Deep Function Calls:");
    let result = deep_call_1();
    print("   Result: " + result);
    print("");
    
    print("2. Nested Conditionals:");
    print("   5 -> " + nested_conditionals(5));
    print("   15 -> " + nested_conditionals(15));
    print("   25 -> " + nested_conditionals(25));
    print("   35 -> " + nested_conditionals(35));
    print("");
    
    print("✓ Stress nesting test completed!");
}
