import "core/token.omni";
import "core/lexer.omni";
import "core/parser.omni";

fn main() {
    let input = "let a = 5;";

    print("--- Starting Parser Test ---");
    print("Input Code:");
    print(input);
    print("-------------------------");

    let l = new_lexer(input);
    let p = new_parser(l);
    
    let ast = p.parse_program();

    print("Generated AST:");
    native "js" {
        console.log(JSON.stringify(ast, null, 2));
    }
}
