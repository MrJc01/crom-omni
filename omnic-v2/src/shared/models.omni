// Shared models used by both server (Python) and client (JS)

export struct User {
    id: i64,
    name: string,
    email: string
}

export struct Message {
    id: i64,
    user_id: i64,
    content: string
}

export fn create_user(id: i64, name: string, email: string) -> User {
    return User { id: id, name: name, email: email };
}
