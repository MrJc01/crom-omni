export fn simple_math(a: i64, b: i64) -> i64 {
    let result = a * 2 + b;
    return result;
}

export fn comparison_test(n: i64) -> bool {
    if (n > 10) {
        return true;
    }
    return false;
}

export fn string_concat(name: string) -> string {
    return "Hello " + name;
}
