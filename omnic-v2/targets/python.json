{
  "name": "python",
  "extension": ".py",
  "comment_single": "#",
  "comment_multi_start": "\"\"\"",
  "comment_multi_end": "\"\"\"",
  "indent": "    ",
  "statement_end": "",
  "block_start": ":",
  "block_end": "",
  "uses_indentation": true,
  
  "templates": {
    "program_header": "# Generated by Omni Compiler\n\n",
    "program_footer": "",
    
    "fn_decl": "def {name}({params}):\n{body}",
    "fn_decl_async": "async def {name}({params}):\n{body}",
    "fn_lambda": "lambda {params}: {expr}",
    
    "let_decl": "{name} = {value}",
    "const_decl": "{name} = {value}",
    "var_decl": "{name} = {value}",
    "typed_decl": "{name}: {type} = {value}",
    
    "if_stmt": "if {condition}:\n{consequence}",
    "if_else_stmt": "if {condition}:\n{consequence}\nelse:\n{alternative}",
    "elif_stmt": "elif {condition}:\n{consequence}",
    "while_stmt": "while {condition}:\n{body}",
    "for_stmt": "for {item} in {iterable}:\n{body}",
    "for_range": "for {item} in range({start}, {end}):\n{body}",
    
    "return_stmt": "return {value}",
    "return_void": "return",
    "pass_stmt": "pass",
    
    "class_decl": "class {name}:\n{body}",
    "class_extends": "class {name}({parent}):\n{body}",
    "constructor": "def __init__(self, {params}):\n{body}",
    "method_decl": "def {name}(self, {params}):\n{body}",
    "method_async": "async def {name}(self, {params}):\n{body}",
    "classmethod": "@classmethod\ndef {name}(cls, {params}):\n{body}",
    "staticmethod": "@staticmethod\ndef {name}({params}):\n{body}",
    "field_assign": "self.{name} = {value}",
    
    "call_expr": "{callee}({args})",
    "method_call": "{object}.{method}({args})",
    "member_access": "{object}.{member}",
    "index_access": "{object}[{index}]",
    "new_expr": "{class}({args})",
    "object_init": "{class}({fields})",
    
    "binary_expr": "{left} {op} {right}",
    "unary_expr": "{op}{operand}",
    "ternary_expr": "{then} if {condition} else {else}",
    "assignment": "{left} = {right}",
    
    "list_literal": "[{elements}]",
    "dict_literal": "{ {pairs} }",
    "dict_pair": "{key}: {value}",
    "tuple_literal": "({elements})",
    "set_literal": "{ {elements} }",
    
    "string_literal": "\"{value}\"",
    "string_fstring": "f\"{value}\"",
    "number_literal": "{value}",
    "bool_true": "True",
    "bool_false": "False",
    "null": "None",
    
    "import_stmt": "import {module}",
    "import_from": "from {module} import {imports}",
    "import_as": "import {module} as {alias}",
    "export_all": "__all__ = [{exports}]",
    
    "comment": "# {text}",
    "docstring": "\"\"\"{text}\"\"\"",
    
    "try_except": "try:\n{try_block}\nexcept {exception} as {error}:\n{except_block}",
    "try_finally": "try:\n{try_block}\nfinally:\n{finally_block}",
    "raise_stmt": "raise {value}",
    
    "await_expr": "await {expr}",
    "with_stmt": "with {expr} as {name}:\n{body}",
    "async_with": "async with {expr} as {name}:\n{body}",
    
    "list_comp": "[{expr} for {item} in {iterable}]",
    "dict_comp": "{{{key}: {value} for {item} in {iterable}}}",
    "generator": "({expr} for {item} in {iterable})",
    
    "decorator": "@{name}",
    "decorator_args": "@{name}({args})",
    
    "spread": "*{expr}",
    "kwargs": "**{expr}"
  },
  
  "type_map": {
    "i8": "int",
    "i16": "int",
    "i32": "int",
    "i64": "int",
    "u8": "int",
    "u16": "int",
    "u32": "int",
    "u64": "int",
    "f32": "float",
    "f64": "float",
    "string": "str",
    "bool": "bool",
    "void": "None",
    "any": "Any"
  },
  
  "operators": {
    "add": "+",
    "sub": "-",
    "mul": "*",
    "div": "/",
    "mod": "%",
    "floordiv": "//",
    "pow": "**",
    "eq": "==",
    "neq": "!=",
    "lt": "<",
    "lte": "<=",
    "gt": ">",
    "gte": ">=",
    "and": "and",
    "or": "or",
    "not": "not ",
    "bitand": "&",
    "bitor": "|",
    "bitxor": "^",
    "bitnot": "~",
    "shl": "<<",
    "shr": ">>"
  }
}
