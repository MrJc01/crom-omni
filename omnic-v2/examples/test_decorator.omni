@ui("MyComponent")
fn MyFunc() {
    print("Inside MyFunc");
}

@ui("MyStruct", width=100)
struct Data {
    x: i64
}

fn main() {
    print("Checking decorators...");
    native "js" {
        console.log("MyFunc decorators:", JSON.stringify(MyFunc.decorators, null, 2));
        console.log("Data decorators:", JSON.stringify(Data.decorators, null, 2));
    }
}
